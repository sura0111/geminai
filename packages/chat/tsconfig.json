{
  "extends": "@geminai/tsconfig",
  "compilerOptions": {
    "declarationDir": "./@types",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.mjs",
    "**/*.cjs",
    "**/*.ts",
    "**/*.tsx",
    "**/*.mts",
    "**/*.cts",
    "**/*.json",
    ".eslintrc.cjs",
  ],
  "exclude": [
    "dist",
    "@types"
  ]
}