{
  "name": "geminai",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "g:rimraf": "cd $INIT_CWD && rimraf",
    "g:eslint": "cd $INIT_CWD && eslint",
    "g:vite-node": "cd $INIT_CWD && vite-node",
    "g:tsc": "cd $INIT_CWD && tsc",
    "g:build": "cd $INIT_CWD && yarn g:rimraf dist && node esbuild.config.mjs && yarn g:tsc",
    "lint": "yarn g:eslint --ext .ts ./",
    "gemini": "yarn workspace @packages/chat run start",
    "build": "nx run-many --target=build --all"
  },
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "devDependencies": {
    "@google/generative-ai": "^0.1.3",
    "@packages/chat": "workspace:^",
    "@packages/env": "workspace:^",
    "@types/eslint": "^8",
    "@types/node": "^20.11.10",
    "@types/prompts": "^2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "axios": "^1.6.7",
    "esbuild": "^0.20.0",
    "esbuild-node-externals": "^1.12.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "https-proxy-agent": "^7.0.2",
    "logdown": "^3.3.1",
    "nx": "17.2.8",
    "prettier": "^3.2.4",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.5",
    "typescript": "*",
    "vite": "^5.0.12",
    "vite-node": "^1.2.2",
    "vitest": "^1.2.2"
  },
  "packageManager": "yarn@4.0.2"
}
